<html>
  <head>
    <link
      href="https://fonts.googleapis.com/css?family=Lato:100,300,400,700,900"
      rel="stylesheet"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Titillium+Web:wght@600&display=swap"
      rel="stylesheet"
    />

    <link rel="stylesheet" href="style.css" />
    <script defer src="/main.js"></script>
  </head>

  <body>
    <div class="bg"></div>

    <div class="top">
      <div class="top_container">
        <div class="top_container2">
          <div class="logo">
            <img src="./img/AddLogo1.png" alt="Address Book Logo" height="80" />
            <p class="tel"></p>
          </div>

          <div class="text_container">
            <div class="text">
              <div class="title">
                <h2>Address Book</h2>

                <div class="search_container">
                  <input
                    class="search_box"
                    type="text"
                    id="main__search_box"
                    name="name"
                  />

                  <button id="main__search_btn" class="search_btn">
                    Search by Name (full text search)
                  </button>

                  <button id="btn_add_client" class="btn_add_cleint">
                    Add Address
                  </button>
                </div>

                <table style="width: 100%" id="t01">
                  <caption>
                    <div class="div"></div>
                    <div class="div"></div>
                  </caption>
                  <thead>
                    <tr>
                      <th>Name</th>
                      <th>Address</th>
                      <th>Postcode</th>
                      <th></th>
                      <th></th>
                    </tr>
                  </thead>
                  <tbody id="table_top" class="customers">
                    <div></div>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
